@prefix : <https://knacc.umbc.edu/leroy/ontologies/trust#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix trust: <https://knacc.umbc.edu/leroy/ontologies/trust#> .
@base <https://knacc.umbc.edu/leroy/ontologies/trust#> .

<https://knacc.umbc.edu/dae-young/kim/ontologies/trust> rdf:type owl:Ontology ;
                                                         owl:versionIRI <https://knacc.umbc.edu/dae-young/kim/ontologies/trust/0.1> .

#################################################################
#    Object Properties
#################################################################

###  https://knacc.umbc.edu/dae-young/kim/ontologies/trust#belongTo
:belongTo rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain trust:Data ;
          rdfs:range trust:Organization .


###  https://knacc.umbc.edu/dae-young/kim/ontologies/trust#hasData
:hasData rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:domain trust:Organization ;
         rdfs:range trust:Data .


###  https://knacc.umbc.edu/dae-young/kim/ontologies/trust#hasEmployed
:hasEmployed rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain trust:Organization ;
             rdfs:range trust:User .


###  https://knacc.umbc.edu/dae-young/kim/ontologies/trust#isAffiliatedWith
:isAffiliatedWith rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdfs:domain trust:User ;
                  rdfs:range trust:Organization .


#################################################################
#    Data properties
#################################################################

###  https://knacc.umbc.edu/leroy/ontologies/trust#behavior
trust:behavior rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf trust:trust ;
               rdfs:domain trust:User ;
               rdfs:range xsd:float .


###  https://knacc.umbc.edu/leroy/ontologies/trust#credibility
trust:credibility rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf trust:veracity ;
                  rdfs:domain trust:Data ;
                  rdfs:range xsd:float .


###  https://knacc.umbc.edu/leroy/ontologies/trust#identity
trust:identity rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf trust:trust ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( trust:Organization
                                           trust:User
                                         )
                           ] ;
               rdfs:range xsd:float .


###  https://knacc.umbc.edu/leroy/ontologies/trust#objectivity
trust:objectivity rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf trust:veracity ;
                  rdfs:domain trust:Data ;
                  rdfs:range xsd:float .


###  https://knacc.umbc.edu/leroy/ontologies/trust#trust
trust:trust rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty .


###  https://knacc.umbc.edu/leroy/ontologies/trust#trustfulness
trust:trustfulness rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf trust:veracity ;
                   rdfs:domain trust:Data ;
                   rdfs:range xsd:float .


###  https://knacc.umbc.edu/leroy/ontologies/trust#veracity
trust:veracity rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf trust:trust ;
               rdfs:domain trust:Data .


#################################################################
#    Classes
#################################################################

###  https://knacc.umbc.edu/leroy/ontologies/trust#Data
trust:Data rdf:type owl:Class ;
           rdfs:comment "Put domain specific data ontology under this Data class." .


###  https://knacc.umbc.edu/leroy/ontologies/trust#Organization
trust:Organization rdf:type owl:Class .


###  https://knacc.umbc.edu/leroy/ontologies/trust#User
trust:User rdf:type owl:Class .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( trust:Data
                trust:Organization
                trust:User
              )
] .


